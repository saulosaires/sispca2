	<queryString>
		<![CDATA[SELECT	uo.sigla, uo.descricao, usr.name, usr.email, usr.cpf, usr.ddd_celular, usr.numero_celular,
	usr.ddd_residencial, usr.numero_residencial, usr.cargo

	FROM controle_acesso.usuario usr
	LEFT JOIN comum.unidade_orcamentaria uo USING (id_unidade_orcamentaria)


	WHERE usr.id_usuario != 214


	ORDER BY 1, 3]]>
