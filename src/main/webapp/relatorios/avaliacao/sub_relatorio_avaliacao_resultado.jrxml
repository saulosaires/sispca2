	<queryString>
		<![CDATA[SELECT topResult.descricao as pergunta, result.resposta

	FROM avaliacao.resultado result
	INNER JOIN avaliacao.exercicio_topico_resultado exercTopResult USING (id_exercicio_topico_resultado)
	INNER JOIN avaliacao.topico_resultado topResult USING (id_topico_resultado)
	INNER JOIN planejamento.exercicio exerc ON (exerc.id_exercicio = result.id_exercicio AND exerc.id_exercicio = exercTopResult.id_exercicio)
	INNER JOIN controle_acesso.usuario usuar USING (id_usuario)
	INNER JOIN planejamento.programa prog USING (id_programa)



	WHERE prog.id_programa = $P!{param_id_prog} AND

	exerc.id_exercicio = $P!{param_id_exerc}


	ORDER BY topResult.id_topico_resultado]]>
	</queryString>
