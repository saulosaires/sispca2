	<queryString>
		<![CDATA[SELECT rec.descricao as recomendacao

	FROM avaliacao.recomendacao rec
	INNER JOIN planejamento.exercicio exerc USING (id_exercicio)
	INNER JOIN controle_acesso.usuario usuar USING (id_usuario)
	INNER JOIN planejamento.programa prog USING (id_programa)


	WHERE prog.id_programa = $P!{param_id_prog}
	AND exerc.id_exercicio = $P!{param_id_exerc}]]>
	</queryString>
