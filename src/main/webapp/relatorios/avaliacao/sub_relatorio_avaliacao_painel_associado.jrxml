	<queryString>
		<![CDATA[SELECT ind.indicador as indicador
FROM avaliacao.painel_associado panelAssoc
	INNER JOIN planejamento.indicadores ind ON (ind.id_indicadores = panelAssoc.id_indicador)
	INNER JOIN planejamento.programa prog USING (id_programa)
   INNER JOIN planejamento.exercicio exerc on  exerc.id_exercicio=panelAssoc.id_exercicio
WHERE prog.id_programa = $P!{param_id_prog}
AND exerc.id_exercicio = $P!{param_id_exerc}]]>
	</queryString>
