	<queryString>
		<![CDATA[SELECT  indDesempInterm.indicador, um.descricao as unidade_medida, indDesempInterm.fonte, indDesempInterm.ref_data_apuracao,
	indDesempInterm.ref_valor, indDesempInterm.resultado_ano_esperado, indDesempInterm.resultado_ano_esperado,
	indDesempInterm.resultado_ano_apurado

	FROM avaliacao.indicador_desempenho_intermediario indDesempInterm

	LEFT JOIN planejamento.unidade_medida um ON (indDesempInterm.id_unidade_medida = um.id_unidade)
	INNER JOIN planejamento.exercicio exerc USING (id_exercicio)
	INNER JOIN controle_acesso.usuario usuar USING (id_usuario)
	INNER JOIN planejamento.programa prog USING (id_programa)

	WHERE prog.id_programa = $P!{param_id_prog} AND exerc.id_exercicio = $P!{param_id_exerc}]]>
	</queryString>
