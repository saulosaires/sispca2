	<queryString>
		<![CDATA[SELECT dir.descricao as diretriz
FROM avaliacao.diretriz_associada dirass
	INNER JOIN planejamento.diretriz dir ON dir.id_diretriz=dirass.id_diretriz
 	INNER JOIN planejamento.programa prog ON prog.id_programa =dirass.id_programa
 	INNER JOIN planejamento.exercicio exerc ON exerc.id_exercicio=dirass.id_exercicio

WHERE prog.id_programa = $P!{param_id_prog}
AND exerc.id_exercicio = $P!{param_id_exerc}]]>
	</queryString>
