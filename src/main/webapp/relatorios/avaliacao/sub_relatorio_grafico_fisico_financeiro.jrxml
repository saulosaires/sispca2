	<queryString>
		<![CDATA[SELECT 	ffm.programa, ffm.ano,

	SUM(ffm.dotacao_inicial) as dotInicial,

	SUM(ffm.disponivel) as dotAtual,

	SUM(ffm.empenhado) as empenhado,

	SUM(ffm.liquidado) as liquidado,

	SUM(ffm.pago) as pago

	FROM siafem.financeiro_mensal ffm

	WHERE ffm.programa = $P{param_cod_prog}

	 and ffm.ano = $P!{param_ano}


	GROUP BY 1, 2]]>
	</queryString>
